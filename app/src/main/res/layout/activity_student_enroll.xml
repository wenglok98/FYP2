<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.studentEnroll">

    <include
        android:layout_height="45dp"
        android:layout_width="match_parent"
        layout="@layout/app_title"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/apptit"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/subject_enroll"
        android:textAlignment="center"
        android:id="@+id/subject_Enrolled"
        android:layout_margin="@dimen/_5sdp"
        android:textSize="@dimen/_16sdp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/apptit"/>

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/subject_Enrolled"
        android:id="@+id/enrolled_Subject_Recycler"/>



</androidx.constraintlayout.widget.ConstraintLayout>